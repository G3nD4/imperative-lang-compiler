var x: integer is 10
var y: real is 3.14
var flag: boolean is true

x := x + 5
y := y * 2.5

if x > 10 then
    flag := false
    print(x)
elseif x = 10 then
    print(0)
else
    print(-1)
end

var a: real is 0.1
var b: real is 1.2

while a < b loop
    print(a)
    a := a + 0.1
end

var result: boolean is (x > 5 and y < 10) or flag

print(result)
